{
  "extends": "airbnb",
  "parser": "babel-eslint",
  rules: {
    "semi": [2, "never"],
    "comma-dangle": [2, "never"],
    "quote-props": [2, "consistent-as-needed"],
    "no-console": 0,
    "react/wrap-multilines": 0,
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0
  }
}
